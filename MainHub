-- ‡∏™‡πà‡∏á‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•
local newdata = game:GetService("HttpService"):JSONEncode(data)
local headers = {["content-type"] = "application/json"}

local request = http_request or request or HttpPost or syn.request or fluxus.request

if request then
    request({
        Url = url, 
        Body = newdata, 
        Method = "POST", 
        Headers = headers
    })
else
    pcall(function()
        game:GetService("HttpService"):PostAsync(url, newdata, Enum.HttpContentType.ApplicationJson, false, headers)
    end)
end
